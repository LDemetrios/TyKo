ignore function

class none()
class auto()

primitive bool = Boolean
primitive int = Long
primitive float = Double
primitive str = String
primitive array<out E> = interface List<E>
primitive dictionary<out V> = interface Map<String, V>

specformat arguments sequence space alignment

class length(
    pts: float,
    em: float,
)

class angle(
    req deg: float,
)

class ratio(
    req value: float,
)

class relative(
    rel: ratio,
    abs: length,
)

class fraction(
    req value: float,
)

